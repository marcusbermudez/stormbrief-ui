{
    "hosting": {
      "public": ".",
      "ignore": [
        "firebase.json",
        "**/node_modules/**",
        "firebase-debug.log"
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            { "key": "X-Robots-Tag", "value": "noindex, nofollow" }
          ]
        }
      ]
    }
  }
  